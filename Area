// area of a recangle //
#include <stdio.h>

int main() {
    int a;
    int b;
    int area;
    printf(" value of the witdh:");
    scanf("%d" ,&a);
    
    printf(" value of the lenth: ");
    scanf("%d", &b);
    area= a*b;
    printf("Area of the rectangle is: %d", area);
    
    return 0;
}
